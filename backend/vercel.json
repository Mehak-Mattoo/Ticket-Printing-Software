{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
   {
  "src": "backend/server.js",
  "use": "@vercel/node"
}
,
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
